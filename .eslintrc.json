{
  "extends": "next",
  "plugins": ["unused-imports"],
  "rules": {
    "react-hooks/exhaustive-deps": "off",
    "no-unused-vars": "off",
    "unused-imports/no-unused-imports": "error",
    "unused-imports/no-unused-vars": ["warn", { "vars": "all", "varsIgnorePattern": "^_", "args": "after-used", "argsIgnorePattern": "^_" }]
  }
}
